{
  "manifest_version": 2,
  "name": "Oscar-Clean",
  "version": "0.1",
  "description": "Make GT OSCAR cleaner for OMSCS students",
  "homepage_url": "http://jsstevenson.github.io",
  "icons": {
    "48": "icons/bookmark-it.png"
  },

  "permissions": [
    "activeTab",
    "tabs"
  ],

  "browser_action": {
    "default_icon": "icons/todo.png",
    "default_title": "Clean OSCAR"
  },

  "background": {
    "scripts": ["background.js"]
  }
}
